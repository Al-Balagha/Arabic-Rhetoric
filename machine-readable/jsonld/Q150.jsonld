        {
            "@id": "wd:Q111",
            "@type": "wikibase:Item",
            "label": {
                "@language": "en",
                "@value": "Fanciful Causation"
            },
            "prefLabel": {
                "@language": "en",
                "@value": "Fanciful Causation"
            },
            "name": {
                "@language": "en",
                "@value": "Fanciful Causation"
            },
            "description": {
                "@language": "en",
                "@value": "Arabic rhetorical device where the communicator denies the obvious cause of something happening, and instead proposes a different but fanciful cause"
            }
        },
        {
            "@id": "wd:Q112",
            "@type": "wikibase:Item",
            "label": {
                "@language": "en",
                "@value": "Rhetorical Concession"
            },
            "prefLabel": {
                "@language": "en",
                "@value": "Rhetorical Concession"
            },
            "name": {
                "@language": "en",
                "@value": "Rhetorical Concession"
            },
            "description": {
                "@language": "en",
                "@value": "Arabic rhetorical device where the communicator appears to concede to an opponent's accusation or argument, only to follow it with a justification, mitigating explanation, or rebuttal"
            }
        },
        {
            "@id": "wd:Q113",
            "@type": "wikibase:Item",
            "label": {
                "@language": "en",
                "@value": "Affirmed Praise"
            },
            "prefLabel": {
                "@language": "en",
                "@value": "Affirmed Praise"
            },
            "name": {
                "@language": "en",
                "@value": "Affirmed Praise"
            },
            "description": {
                "@language": "en",
                "@value": "Arabic rhetorical device where the communicator praises, then indicates that there is an exception to the praise, and then affirms the original praise by praising again"
            }
        },
        {
            "@id": "wd:Q114",
            "@type": "wikibase:Item",
            "label": {
                "@language": "en",
                "@value": "Affirmed Dispraise"
            },
            "prefLabel": {
                "@language": "en",
                "@value": "Affirmed Dispraise"
            },
            "name": {
                "@language": "en",
                "@value": "Affirmed Dispraise"
            },
            "description": {
                "@language": "en",
                "@value": "Arabic rhetorical device where the communicator criticises, then indicates that there is an exception to the criticism, and then affirms the original criticism by criticising again"
            }
        },
        {
            "@id": "wd:Q115",
            "@type": "wikibase:Item",
            "label": {
                "@language": "en",
                "@value": "Affirmation by Negation"
            },
            "prefLabel": {
                "@language": "en",
                "@value": "Affirmation by Negation"
            },
            "name": {
                "@language": "en",
                "@value": "Affirmation by Negation"
            },
            "description": {
                "@language": "en",
                "@value": "Arabic rhetorical device where the communicator alludes to a word by negating its antonym, for emphasis, praise, or denial"
            }
        },
        {
            "@id": "wd:Q116",
            "@type": "wikibase:Item",
            "label": {
                "@language": "en",
                "@value": "Praise-Blame Dichotomy"
            },
            "prefLabel": {
                "@language": "en",
                "@value": "Praise-Blame Dichotomy"
            },
            "name": {
                "@language": "en",
                "@value": "Praise-Blame Dichotomy"
            },
            "description": {
                "@language": "en",
                "@value": "Arabic rhetorical device in which a subject is praised after being criticized, or criticized after being praised, enhancing the emotional impact, sharpening the tone, and revealing layered meaning"
            }
        },
        {
            "@id": "wd:Q117",
            "@type": "wikibase:Item",
            "label": {
                "@language": "en",
                "@value": "Feigned Ignorance"
            },
            "prefLabel": {
                "@language": "en",
                "@value": "Feigned Ignorance"
            },
            "name": {
                "@language": "en",
                "@value": "Feigned Ignorance"
            },
            "description": {
                "@language": "en",
                "@value": "Arabic rhetorical device where the communicator knows something but pretends not to know, thus expressing amazement, praise, dispraise, disapproval, or to reprimand, or create a sense of familiarity"
            }
        },
        {
            "@id": "wd:Q118",
            "@type": "wikibase:Item",
            "label": {
                "@language": "en",
                "@value": "Diminishing Expression"
            },
            "prefLabel": {
                "@language": "en",
                "@value": "Diminishing Expression"
            },
            "name": {
                "@language": "en",
                "@value": "Diminishing Expression"
            },
            "description": {
                "@language": "en",
                "@value": "Arabic rhetorical device in which the communicator intentionally uses language that minimizes, devalues, or belittles a person, object, or idea to make it seem less significant, impressive, or worthy"
            }
        },
        {
            "@id": "wd:Q119",
            "@type": "wikibase:Item",
            "label": {
                "@language": "en",
                "@value": "Addressing the Non-Human"
            },
            "prefLabel": {
                "@language": "en",
                "@value": "Addressing the Non-Human"
            },
            "name": {
                "@language": "en",
                "@value": "Addressing the Non-Human"
            },
            "description": {
                "@language": "en",
                "@value": "Arabic rhetorical device involving a form of personification where the communicator directly addresses a non-human object as if it was human"
            }
        },
        {
            "@id": "wd:Q120",
