        {
            "@id": "wd:Q90",
            "@type": "wikibase:Item",
            "label": {
                "@language": "en",
                "@value": "Finesse of Initiation"
            },
            "prefLabel": {
                "@language": "en",
                "@value": "Finesse of Initiation"
            },
            "name": {
                "@language": "en",
                "@value": "Finesse of Initiation"
            },
            "description": {
                "@language": "en",
                "@value": "Arabic rhetorical device where the communicator starts the text in an excellent manner which hints at the main objective of the text, attracts the attention of the addressee and makes the addressee eager to hear the main proposition"
            }
        },
        {
            "@id": "wd:Q91",
            "@type": "wikibase:Item",
            "label": {
                "@language": "en",
                "@value": "Similarities of the Start & Finish"
            },
            "prefLabel": {
                "@language": "en",
                "@value": "Similarities of the Start & Finish"
            },
            "name": {
                "@language": "en",
                "@value": "Similarities of the Start & Finish"
            },
            "description": {
                "@language": "en",
                "@value": "Arabic rhetorical device where the end of a sentence has a similar meaning to the beginning of the following sentence, establishing a semantic link which bridges the two sentences"
            }
        },
        {
            "@id": "wd:Q92",
            "@type": "wikibase:Item",
            "label": {
                "@language": "en",
                "@value": "Parallelism"
            },
            "prefLabel": {
                "@language": "en",
                "@value": "Parallelism"
            },
            "name": {
                "@language": "en",
                "@value": "Parallelism"
            },
            "description": {
                "@language": "en",
                "@value": "Arabic rhetorical device where the communicator repeats the lexical, syntactic, or rhythmic structure of a sentence or phrase across two or more phrases, clauses, or sentences to create semantic balance, assonance, rhyme, or rhetorical emphasis"
            }
        },
        {
            "@id": "wd:Q93",
            "@type": "wikibase:Item",
            "label": {
                "@language": "en",
                "@value": "Digression"
            },
            "prefLabel": {
                "@language": "en",
                "@value": "Digression"
            },
            "name": {
                "@language": "en",
                "@value": "Digression"
            },
            "description": {
                "@language": "en",
                "@value": "Arabic rhetorical device where the communicator mentions one topic, then leaves that topic and mentions something else which may be the opposite or contrasting to the original topic, then returns to complete the original topic"
            }
        },
        {
            "@id": "wd:Q94",
            "@type": "wikibase:Item",
            "label": {
                "@language": "en",
                "@value": "Rhetorical Branching"
            },
            "prefLabel": {
                "@language": "en",
                "@value": "Rhetorical Branching"
            },
            "name": {
                "@language": "en",
                "@value": "Rhetorical Branching"
            },
            "description": {
                "@language": "en",
                "@value": "Arabic rhetorical device where the validity of a new statement is derived from a previous one, serving as a justification for a change in topic that favours the communicator's objective"
            }
        },
        {
            "@id": "wd:Q95",
            "@type": "wikibase:Item",
            "label": {
                "@language": "en",
                "@value": "Change of Topic"
            },
            "prefLabel": {
                "@language": "en",
                "@value": "Change of Topic"
            },
            "name": {
                "@language": "en",
                "@value": "Change of Topic"
            },
            "description": {
                "@language": "en",
                "@value": "Arabic rhetorical device where the communicator transitions from the introduction to the main point of the text in a smooth, connected, integrated and subtle manner"
            }
        },
        {
            "@id": "wd:Q96",
            "@type": "wikibase:Item",
            "label": {
                "@language": "en",
                "@value": "Forewarning"
            },
            "prefLabel": {
                "@language": "en",
                "@value": "Forewarning"
            },
            "name": {
                "@language": "en",
                "@value": "Forewarning"
            },
            "description": {
                "@language": "en",
                "@value": "Arabic rhetorical device where the communicator subtly prepares the audience by providing preliminary hints, enabling them to anticipate the message before it is explicitly delivered"
            }
        },
        {
            "@id": "wd:Q97",
            "@type": "wikibase:Item",
            "label": {
                "@language": "en",
                "@value": "Anticipatory Closure"
            },
            "prefLabel": {
                "@language": "en",
                "@value": "Anticipatory Closure"
            },
            "name": {
                "@language": "en",
                "@value": "Anticipatory Closure"
            },
            "description": {
                "@language": "en",
                "@value": "Arabic rhetorical device where the comminicator introduces an early phrase that guides or prepares the listener for the final word of a clause or rhyme of the verse, thus enhancing rhythmic harmony and closure"
            }
        },
        {
            "@id": "wd:Q98",
            "@type": "wikibase:Item",
            "label": {
                "@language": "en",
                "@value": "Pleasantness of the Ending"
            },
            "prefLabel": {
                "@language": "en",
                "@value": "Pleasantness of the Ending"
            },
            "name": {
                "@language": "en",
                "@value": "Pleasantness of the Ending"
            },
            "description": {
                "@language": "en",
                "@value": "Arabic rhetorical device where the communicator concludes the text in a manner that is pleasing for the addressee from a lexical viewpoint"
            }
        },
        {
            "@id": "wd:Q99",
            "@type": "wikibase:Item",
            "label": {
                "@language": "en",
                "@value": "Finesse of the Ending"
            },
            "prefLabel": {
                "@language": "en",
                "@value": "Finesse of the Ending"
            },
            "name": {
                "@language": "en",
                "@value": "Finesse of the Ending"
            },
            "description": {
                "@language": "en",
                "@value": "Arabic rhetorical device where the communicator concludes the text in a way that links to, and summarises the objective of the text, and brings a natural and satisfying conclusion to the text, from a semantical viewpoint"
            }
        },
