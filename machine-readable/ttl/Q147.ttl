wd:Q82 a wikibase:Item ;
	rdfs:label "Syntactic Grouping"@en ;
	skos:prefLabel "Syntactic Grouping"@en ;
	schema:name "Syntactic Grouping"@en ;
	schema:description "Arabic rhetorical device where several things are collected together and a collective verdict or judgement is made"@en .

wd:Q83 a wikibase:Item ;
	rdfs:label "Semantic Grouping"@en ;
	skos:prefLabel "Semantic Grouping"@en ;
	schema:name "Semantic Grouping"@en ;
	schema:description "Arabic rhetorical device where multiple, often related or similar, qualities or details are grouped together to enhance emphasis or create a rich, detailed picture"@en .

wd:Q84 a wikibase:Item ;
	rdfs:label "Dividing Items"@en ;
	skos:prefLabel "Dividing Items"@en ;
	schema:name "Dividing Items"@en ;
	schema:description "Arabic rhetorical device where a general category or statement is broken down into its constituent parts, and each part is assigned a specific trait, action, or outcome, create emphasis, rhythm, or clarity through structured elaboration"@en .

wd:Q85 a wikibase:Item ;
	rdfs:label "Grouping Then Dividing"@en ;
	skos:prefLabel "Grouping Then Dividing"@en ;
	schema:name "Grouping Then Dividing"@en ;
	schema:description "Arabic rhetorical device where a general collective statement is made first (jam‘), and then this is divided or detailed (taqsīm)"@en .

wd:Q86 a wikibase:Item ;
	rdfs:label "Grouping Then Differentiating"@en ;
	skos:prefLabel "Grouping Then Differentiating"@en ;
	schema:name "Grouping Then Differentiating"@en ;
	schema:description "Arabic rhetorical device where the communicator first combines multiple subjects under a shared predicate, and then differentiates them with individual qualifiers or extensions"@en .

wd:Q87 a wikibase:Item ;
	rdfs:label "Differentiating Between Similar Items"@en ;
	skos:prefLabel "Differentiating Between Similar Items"@en ;
	schema:name "Differentiating Between Similar Items"@en ;
	schema:description "Arabic rhetorical device involving differentiation between two things that mistakenly could be considered to be same"@en .

wd:Q88 a wikibase:Item ;
	rdfs:label "Folding and Unfolding"@en ;
	skos:prefLabel "Folding and Unfolding"@en ;
	schema:name "Folding and Unfolding"@en ;
	schema:description "Arabic rhetorical device where two or more items are mentioned in succession  (اللفّ), then, further information is \"Unfolded\" (النشر) about these items"@en .

