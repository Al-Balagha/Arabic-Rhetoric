	rdfs:label "Tail-Head"@en ;
	skos:prefLabel "Tail-Head"@en ;
	schema:name "Tail-Head"@en ;
	schema:description "Arabic rhetorical device where the last word of the sentence (the \"tail\", al-‘ajz) and the first word of the sentence (the \"head\", al-ṣadr) are the same or morphologically related"@en .

