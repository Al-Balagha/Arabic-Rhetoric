	<rdf:Description rdf:about="https://balaghabase.org/entity/Q78">
		<rdf:type rdf:resource="http://wikiba.se/ontology#Item"/>
		<rdfs:label xml:lang="en">Evasive Letterplay</rdfs:label>
		<skos:prefLabel xml:lang="en">Evasive Letterplay</skos:prefLabel>
		<schema:name xml:lang="en">Evasive Letterplay</schema:name>
		<schema:description xml:lang="en">Arabic rhetorical device where the communicator intentionally uses a word which can change its meaning through a small change in the word itself</schema:description>
	</rdf:Description>
	<rdf:Description rdf:about="https://balaghabase.org/entity/Q79">
		<rdf:type rdf:resource="http://wikiba.se/ontology#Item"/>
		<rdfs:label xml:lang="en">Euphemism</rdfs:label>
		<skos:prefLabel xml:lang="en">Euphemism</skos:prefLabel>
		<schema:name xml:lang="en">Euphemism</schema:name>
		<schema:description xml:lang="en">Arabic rhetorical device where the communicator uses an implicit reference rather than explicitly referring to something unpleasant or embarrassing</schema:description>
	</rdf:Description>
	<rdf:Description rdf:about="https://balaghabase.org/entity/Q80">
		<rdf:type rdf:resource="http://wikiba.se/ontology#Item"/>
		<rdfs:label xml:lang="en">Referential Shift</rdfs:label>
		<skos:prefLabel xml:lang="en">Referential Shift</skos:prefLabel>
		<schema:name xml:lang="en">Referential Shift</schema:name>
		<schema:description xml:lang="en">Arabic rhetorical device where the communicator mentions something, and then later uses a personal pronoun or demonstrative pronoun to apparently refer back to that thing although something related but different is intended by the pronoun</schema:description>
	</rdf:Description>
