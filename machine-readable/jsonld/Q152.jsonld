            "prefLabel": {
                "@language": "en",
                "@value": "Metrical Ellipsis"
            },
            "name": {
                "@language": "en",
                "@value": "Metrical Ellipsis"
            },
            "description": {
                "@language": "en",
                "@value": "Arabic rhetorical device where the rhyme and or poetic metre of a poem is such that even if some words are omitted, the rhyme or metre are maintained"
            }
        },
